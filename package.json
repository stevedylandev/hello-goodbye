{
	"name": "hello-goodbye",
	"version": "0.0.0",
	"private": true,
	"description": "Monorepo workspace for hello projects",
	"type": "module",
	"packageManager": "pnpm@9.15.2",
	"scripts": {
		"changeset": "changeset",
		"bump:stable": "changeset version",
		"bump:next": "changeset version --snapshot next",
		"bump:preview": "changeset version --snapshot preview",
		"release:stable": "changeset publish",
		"release:next": "changeset publish --no-git-tag --snapshot --tag next",
		"release:preview": "changeset publish --no-git-tag --snapshot --tag preview",
		"packages:prepublish": "pnpm -r prepublish"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.2",
		"@changesets/cli": "^2.29.8"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"engines": {
		"node": ">=14"
	}
}
